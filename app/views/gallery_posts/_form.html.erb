<%= form_for @GalleryPost do |f| %>

<%= f.label :name%>
<%= f.text_field :name%>

<%= f.label :image%>
<%= f.file_field :image %>

<%= f.label :sequence%>
<%= f.select :sequence, GalleryPost.all.map {|c| c.sequence}, include_blank: "add on the end (max seq)" %>

<%= f.label :from_left%>
<%= f.text_field :from_left   %>

<%= f.label :height, "Height px" %>
<%= f.text_field :height   %>

<%= f.submit %>
<%end %>