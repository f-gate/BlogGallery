<h1><%= @blog.title %></h1>
<h2><%= @blog.category %></h2>

<h3><%= @blog.content %></h3>

<div style= "width: 200px, height: 100px"> 
    <% if @blog.main_photo.attached? %>
    <image src="<%=(url_for(@blog.main_photo))%>">
    <% end %>
</div>

<%=  link_to 'edit', edit_blog_path(@blog.id) %>
<%=  link_to 'delete', blog_path(@blog.id), method: :delete %>

