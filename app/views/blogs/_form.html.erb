<%= form_for @blog do |f| %>

<%= f.label :title%>
<%= f.text_field :title%>

<%= f.label :content%>
<%= f.text_area :content%>

<%= f.file_field :main_photo %>

<%= select_tag 'category', options_for_select(Category.all, "id", "category") %>
<%= f.submit %>

<%= @blog.errors.full_messages %>

<%end %>